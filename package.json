{
  "name": "niconico-on-mac",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "repository": "https://github.com/hirokihello/niconico-on-mac",
  "keywords": [
    "demo"
  ],
  "scripts": {
    "start-electron": "electron .",
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn build && tsc -p electron && electron-builder"
  },
  "author": "hirokihello",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^19.0.4",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-solid": "^2.2.6"
  },
  "dependencies": {
    "concurrently": "^7.2.1",
    "electron-builder": "^23.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^8.0.2",
    "solid-js": "^1.4.2",
    "wait-on": "^6.0.1"
  }
}
